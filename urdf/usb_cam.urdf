<?xml version="1.0"?>
<!-- Kinematic model, simplified visual, no collisions -->
<robot name="usb_cam" xmlns:xacro="http://www.ros.org/wiki/xacro">
	<link name="base_link"/>
	<link name="optical_frame"/>
	<joint name="optical_frame_to_base" type="fixed">
		<parent link="base_link"/>
		<child link="optical_frame"/>
	</joint>
</robot>
