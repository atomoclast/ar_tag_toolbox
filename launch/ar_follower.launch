<launch>
  
  <node pkg="ar_tag_toolbox" name="ar_follower" type="ar_tag_tracker.py" clear_params="true" output="screen">
    <rosparam>
       rate: 10
       max_x: .3
       goal_x: 0.15
       x_threshold: 0.1
       y_threshold: 0.05
       y_scale: 2.0
       x_scale: 1.0
       max_angular_speed: 0
       min_angular_speed: 0
       max_linear_speed: 0.5
       min_linear_speed: 0.05
    </rosparam>
    
  </node>
</launch>
